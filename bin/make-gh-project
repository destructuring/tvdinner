#!/bin/bash -e

nm_repo=$1; shift

mkdir $nm_repo # fail if directory already exists
cd $nm_repo
git init
touch README.mkd
git add README.mkd
git commit -m 'new project'
hub create
git push -u origin master

git remote add gs https://github.com/HeSYINUvSBZfxqA/getting-started
git fetch gs
git merge gs/master

bash
